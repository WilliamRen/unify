{
  "name": "unify",
  "kind" : "full",
  "package" : "",
  "fields" : {
    "application" : {
      "check" : "String",
      "default" : "default"
    },
    "application.version" : {
      "check" : "String",
      "default" : "none"
    },
    "os.name" : {
      "check" : ["win", "mac", "unix", "android", "ios", "webos", "qnx"],
      "detect" : "unify.bom.detect.OsName"
    }
  },
  "requires" : [{
    "source" : "https://github.com/fastner/lowland.git"
  }, {
    "source" : "https://github.com/fastner/jasy-appcache.git"
  }]
}
